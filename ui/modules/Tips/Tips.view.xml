<mvc:View
	controllerName="tag.rugby.ui.modules.Tips.Tips"
    xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	height="100%">

	<Page
		title="Rules"
		class="sapUiContentPadding" >
        <customHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="Tips for playing TAG Rugby" level="H2"/>
				<ToolbarSpacer/>
			</Toolbar>
		</customHeader>
		<content>
            <VBox class="sapUiMediumMargin">
                <FormattedText htmlText="{/HTML}"/>
            </VBox>
		</content>
	</Page>
</mvc:View>