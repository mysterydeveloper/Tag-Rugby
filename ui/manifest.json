{
  "sap.app": {
    "id": "tag.rugby.ui",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui5": {
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "tag.rugby.ui.i18n.i18n"
        }
      }
    },
    "dependencies": {
      "minUI5Version": "1.98.0"
    },
    "rootView": {
      "viewName": "tag.rugby.ui.modules.App",
      "type": "XML",
      "async": true
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "path": "tag.rugby.ui.modules",
        "controlId": "rootControl",
        "controlAggregation": "pages",
        "type": "View",
        "viewType": "XML",
        "async": true
      },
      "routes": [{
          "name": "Home",
          "pattern": "",
          "target": "Home"
        },
        {
          "name": "Register",
          "pattern": "Register",
          "target": "Register"
        },
        {
          "name": "Teams",
          "pattern": "Teams",
          "target": "Teams"
        },
        {
          "name": "Rules",
          "pattern": "Rules",
          "target": "Rules"
        },
        {
          "name": "Plays",
          "pattern": "Plays",
          "target": "Plays"
        },
        {
          "name": "Admin",
          "pattern": "Admin",
          "target": "Admin"
        },
        {
          "name": "Players",
          "pattern": "Players",
          "target": "Players"
        },
        {
          "name": "Events",
          "pattern": "Events",
          "target": "Events"
        },
        {
          "name": "TeamsPicking",
          "pattern": "TeamsPicking",
          "target": "TeamsPicking"
        },
        {
          "name": "Login",
          "pattern": "Login/{previousPage}",
          "target": "Login"
        }
      ],
      "targets": {
        "Home": {
          "name": "Home.Home"
        },
        "Register": {
          "name": "Register.Register"
        },
        "Teams": {
          "name": "Teams.Teams"
        },
        "TeamsPicking": {
          "name": "TeamsPicking.TeamsPicking"
        },
        "Rules": {
          "name": "Rules.Rules"
        },
        "Plays": {
          "name": "Plays.Plays"
        },
        "Admin": {
          "name": "Admin.Admin"
        },
        "Players": {
          "name": "Players.Players"
        },
        "Events": {
          "name": "Events.Events"
        },
        "Login": {
          "name": "Login.Login"
        }
      }
    }
  }
}
